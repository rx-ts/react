{
  "name": "react-qrious",
  "version": "1.0.4",
  "description": "a React component of generating qrcode with `qrious`",
  "module": "lib/react-qrious",
  "main": "dist/react-qrious",
  "unpkg": "dist/react-qrious",
  "browser": "dist/react-qrious",
  "repository": "git@github.com:JounQin/react-qrious.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "demo": "rollup -c rollup.demo-config.js",
    "dev": "yarn demo -- --watch",
    "client": "rollup -c",
    "server": "cross-env ENV=server rollup -c",
    "build": "yarn client && yarn server",
    "build:prod": "cross-env NODE_ENV=production yarn build",
    "dist": "yarn build && yarn build:prod",
    "lint": "eslint . --max-warnings=10",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "yarn dist"
  },
  "files": [
    "dist/*.js",
    "lib/*.js"
  ],
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "node-qrious": "^4.0.0",
    "qrious": "^4.0.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-config-1stg": "^0.0.5",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  }
}
